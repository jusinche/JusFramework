@model JusNucleo.Bl.Personas.PersonaNatural
@{
    Layout = "~/Views/Shared/SubLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">

        @using (Html.BeginForm("Create", "PersonaNatural", FormMethod.Post, new Dictionary<string, object> { { "onsubmit", "return validarFormulario(this);" }, { "data-toggle", "validator" }, { "id", "CrearPersona" } }))
        {
            @Html.Partial("_Form")

            <div class="text-center">

                <input type="submit" value="Guardar" class="btn btn-primary"/>
                @Html.ActionLink("Salir", "Index", "PersonaNatural", new { @class = "btn btn-default", id = "btnCrearPersona" })
            </div>
        }
    </div>
</div>

