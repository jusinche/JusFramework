@model JusNucleo.Bl.Personas.PersonaNatural
@{
    Layout = "~/Views/Shared/SubLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        @using (Html.BeginForm("Index", "PersonaNatural", FormMethod.Post, new Dictionary<string, object> { { "data-toggle", "validator" }, { "id", "EditarPersona" } }))
        {
            @Html.Partial("_Form")

            <div class="text-center">
                <input type="submit" value="Guardar" class="btn btn-primary"/>
                @Html.ActionLink("Cancelar", "Index", "PersonaNatural", new {@class = "btn btn-default", id="btnEditarPersona"})
            </div>
        }
    </div>
</div>
<script>
    $(function () {
        $("#btnEditarPersona").click(function () {
            if (!$("#EditarPersona").valid()) {
                return false;
            }
            return true;
        });
    });
</script>